<div class='country__card'>
  <h2 class='contry__name'>
    {{name}}
  </h2>
  <svg></svg>
  <p class='country__capital'>
    Capital:{{capital}}
  </p>
  <p class='contry__population'>
    Population:{{population}}
  </p>
  <p class='country__languages-container'>
    Languages:
  </p>
  <ul class='country__languages-list'>
    {{!-- {{#each this}} --}}
    <li class='country__language'></li>
    {{!-- {{#each}} --}}
  </ul>
</div>